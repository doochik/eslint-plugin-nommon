{
  "name": "eslint-plugin-nommon",
  "version": "2.1.1",
  "description": "ESLint plugin for nommon",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "nommon",
    "jpath"
  ],
  "author": "Aleksei Androsov",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint .",
    "unit": "mocha tests --recursive",
    "test": "npm run lint && npm run unit"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "eslint-config-eslint": "^11.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^11.0.1"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doochik/eslint-plugin-nommon.git"
  },
  "bugs": {
    "url": "https://github.com/doochik/eslint-plugin-nommon/issues"
  },
  "homepage": "https://github.com/doochik/eslint-plugin-nommon#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run unit"
    ]
  }
}
